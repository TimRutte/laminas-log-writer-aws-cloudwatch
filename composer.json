{
    "name": "timrutte/laminas-log-writer-cloudwatch",
    "description": "Log writer to send all logs to AWS Cloudwatch",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "log",
        "logging"
    ],
    "homepage": "https://www.timrutte.de",
    "support": {
        "issues": "https://github.com/timrutte/laminas-log-writer-cloudwach/issues",
        "source": "https://github.com/timrutte/laminas-log-writer-cloudwach",
        "rss": "https://github.com/timrutte/laminas-log-writer-cloudwach/releases.atom"
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "aws/aws-sdk-php": "^3.33",
        "laminas/laminas-log": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "TimRutte\\Laminas\\Log\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TimRutteTest\\Laminas\\Log\\": "test/"
        },
        "files": [
            "test/Writer/TestAsset/chmod.php"
        ]
    }
}
